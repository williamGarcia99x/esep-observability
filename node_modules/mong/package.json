{
  "name": "mong",
  "version": "2.0.2",
  "description": "Lightweight models for mongodb objects",
  "main": "lib/index",
  "scripts": {
    "lint": "standard",
    "test": "mocha --bail --recursive test/",
    "coverage": "mkdir -p coverage && mocha --recursive -r blanket -R html-cov --recursive test/ > coverage/index.html",
    "travis": "mocha -r blanket -R mocha-lcov-reporter --recursive test/ | coveralls"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aantthony/mong.git"
  },
  "engines": {
    "node": ">=0.11"
  },
  "keywords": [
    "mongodb",
    "document",
    "model",
    "database",
    "odm",
    "orm",
    "nosql"
  ],
  "author": "Anthony Foster <git@af.id.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aantthony/mong/issues"
  },
  "dependencies": {
    "es6-promise": "^2.3.0",
    "map-stream": "0.0.6",
    "mongodb": "^2.0.46"
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.3",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "should": "^7.0.2",
    "sinon": "~1.7.3",
    "standard": "^5.3.1"
  }
}
